programa
{
inteiro num
inteiro cen
inteiro dez 
inteiro uni
	funcao inicio()
	{
		escreva("Escreva um numero de 100 a 999", "\n")
		leia
(num)
		se((num<100) ou (num>999)){
		escreva("você não pode fazer esse calulo", "\n")
		}
		senao{
		cen=num/100
		dez=(num%100)/10
		uni=num%10
		escreva("A casa da centena ",cen, "\n")
		escreva("A casa da dezena ",dez, "\n")
		escreva("A casa da unidade ",uni, "\n")
		}
	
	}
}

