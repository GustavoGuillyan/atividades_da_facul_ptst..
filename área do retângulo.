programa
{
real l1
real l2
real area

	
	funcao inicio()
	{
		escreva("Esse algaritmo calcula a area de um retângulo. ", "\n")
		escreva("Digite o tamanho da base o retângulo: ")
		leia(l1)
		escreva("Digite o tamanho da altura do retângulo; ")
		leia(l2)
		se(l1!=l2){
			area=l1*l2
			escreva("A area do retângulo é ", area)
			}
			senao{
				escreva("Esse poligino não é um retângulo.")
				}	
	}
}
